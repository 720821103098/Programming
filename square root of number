//square root of number
#include <math.h>
#include <stdio.h>
int main() 
{
   double number, squareRoot;
   printf("Enter a number: ");
   scanf("%lf", &number);
   squareRoot = sqrt(number);
   printf("Square root of %.2lf =  %.2lf", number, squareRoot); 
   retURN 0;
}
 
OUTPUT:
Enter a number: 49
Square root of 49.00 =  7.00
